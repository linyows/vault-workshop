extends layout

block content
  h1= title

  form(action="/users/create", method="post")
    div(class="form-group")
      label(for="username")
        | Username
      input(type="text", name="username")
    div(class="form-group")
      label(for="email")
        | Email
      input(type="text", name="email")
    input(type="submit" class="btn btn-default")
